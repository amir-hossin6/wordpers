{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Summary",
	"slug": "text-summary",
	"blockTypes": [
		"core/heading",
		"core/paragraph",
		"core/post-date",
		"core/post-excerpt",
		"core/post-title",
		"core/site-title",
		"core/site-tagline",
		"core/query-title",
		"core/term-description"
	],
	"styles": {
		"typography": {
			"fontSize": "var(--wp--preset--font-size--tiny)",
			"lineHeight": "var(--wp--custom--global--line-height--xx-large)"
		}
	}
}
